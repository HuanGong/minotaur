<config>
  <io_service
    fd_count="65535"
    event_loop_worker="1"
    io_queue_size="131072"
    io_worker="2"
    service_queue_size="131072"
    service_worker="2"
    stack_size="16384" />

  <services>
    <service address="http://0.0.0.0:6600" name="http_echo_handler"/>
  </services>

  <clients>
    <router name="rapid">
      <client address="rapid://0.0.0.0:6601" timeout="1000" count="5"/>
    </router>
  </clients>

</config>
